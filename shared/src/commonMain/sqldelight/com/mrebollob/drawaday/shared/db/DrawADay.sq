
CREATE TABLE DrawImage(
id TEXT NOT NULL PRIMARY KEY,
title TEXT NOT NULL,
drawing TEXT NOT NULL,
source TEXT NOT NULL,
publish_date TEXT NOT NULL
);

insertItem:
INSERT OR REPLACE INTO DrawImage(id, title, drawing, source, publish_date)VALUES(?, ?, ?, ?, ?);

selectAll:
SELECT * FROM DrawImage;

selectById:
SELECT *
FROM DrawImage
WHERE id = ?;

deleteAll:
DELETE FROM DrawImage;